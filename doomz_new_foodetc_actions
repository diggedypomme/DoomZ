  Scale 0.12
 +INVENTORY.UNDROPPABLE
 Inventory.MaxAmount 100
  Obituary "$OB_MPPISTOL"
  //+WEAPON.WIMPY_WEAPON

  Weapon.SlotNumber 9 
  States
  {
