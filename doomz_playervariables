

ACTOR Dectemperature : Ammo                                                                           
{
 Inventory.Amount 100 // You'll get half this if a monster drops it.
 Inventory.MaxAmount 100
 Ammo.BackpackAmount 0 // How much you'll get out of backpacks or equivalent.
 Ammo.BackpackMaxAmount 100 // The new max amount if player has a backpack.
 Inventory.Icon "CLIPA0" // This displays on the fullscreen HUD.
 Inventory.PickupMessage "You got a bit warmer."
 States
 {
 Spawn:
  BOLB A -1
  Stop
  //Death:
  //  BOLB A 160
 }
}
