
actor till 10804
{
	//till for the shop. By Burtross - DoomZ

	+NOGRAVITY
	Radius 10
	Height 10
	+SOLID
	+SHOOTABLE
	+NODAMAGE
	+NOBLOOD
	+CANPASS
	scale 1.5
	States
	{
	Spawn:
		TILL A -1
		Stop
	}
}



